public class KDnode{
    Word word;
    KDnode left;
    KDnode right;
    int depth;

    public KDnode(){
    	this.word = null;
    	this.left = null;
    	this.right = null;
    }

    public KDnode(Word word, int depth) {
        this.word = word;
        this.depth = depth;
        this.left = null;
    	this.right = null;
    }
}
